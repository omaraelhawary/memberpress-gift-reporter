{
  "name": "memberpress-gift-reporter",
  "version": "1.3.0",
  "description": "A WordPress plugin that generates comprehensive reports for the MemberPress Gifting add-on with advanced filtering, showing the linkage between gift givers and recipients.",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "npm run build:css && npm run build:js",
    "build:css": "cleancss -o assets/css/style.min.css assets/css/style.css",
    "build:js": "uglifyjs assets/js/script.js -o assets/js/script.min.js",
    "prepack": "npm run build",
    "dist": "./build.sh",
    "package": "npm run build && ./build.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omaraelhawary/memberpress-gift-reporter.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/omaraelhawary/memberpress-gift-reporter/issues"
  },
  "homepage": "https://github.com/omaraelhawary/memberpress-gift-reporter#readme",
  "devDependencies": {
    "clean-css-cli": "^5.6.3",
    "uglify-js": "^3.19.3"
  }
}